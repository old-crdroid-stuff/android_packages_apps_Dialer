    <string name="call_recording_failed_message">அழைப்புப் பதிவித்தல் தொடங்குவதில் தோற்றது</string>
    <string name="call_recording_file_location">அழைப்புப்பதிவு <xliff:g id="filename">%s</xliff:g> இல் சேமிக்கப்பட்டது</string>
    <string name="onscreenCallRecordText">அழைப்பைப் பதிவுசெய்க</string>
